#!/usr/bin/perl
#
# The main driver for the Ciltut system.
#

use strict;

use FindBin;
use lib "$FindBin::RealBin/../..";
use lib "$FindBin::RealBin/../lib";
use lib "$FindBin::RealBin/../cil/bin";
use lib "$FindBin::RealBin/../cil/lib";

use CilConfig;
use Ciltut;

$::ciltuthome = "$FindBin::RealBin/..";

Ciltut->new(@ARGV)->doit();

exit(0);
